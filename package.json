{
  "name": "@bensandee/node-win-rw",
  "description": "Node.js Win-RW Bindings",
  "version": "0.0.3",
  "homepage": "https://github.com/bensandee/node-win-rw",
  "author": {
    "name": "Caylan Larson",
    "url": "https://semireg.com/",
    "email": "caylan@semireg.com"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/bensandee/node-win-rw.git"
  },
  "devDependencies": {
    "nodeunit": "*",
    "prebuild": "^12.1.0",
    "prebuild-ci": "^4.0.0",
    "node-abi": "^3.51.0",
    "node-gyp": "^10.0.1",
    "patch-package": "^6.4.7"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "apply-patches": "patch-package",
    "prebuild-all": "prebuild --all --force --strip --verbose --openssl_fips=X",
    "prebuild": "prebuild",
    "rebuild": "node-gyp rebuild",
    "test": "nodeunit test"
  },
  "binary": {
    "module_name": "win_rw",
    "module_path": "./lib/",
    "host": "https://github.com/semireg/win-rw/releases/download/",
    "remote_path": "v{version}"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "main": "./lib/win-rw",
  "dependencies": {
    "nan": "^2.17.0",
    "prebuild-install": "^7.1.1"
  }
}
